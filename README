sudo apt-get install qt5-default
sudo apt-get install libavdevice-dev libavfilter-dev libavformat-dev libavcodec-dev libswresample-dev libswscale-dev libavutil-dev --install-recommends
sudo apt-get install libvdpau-dev libva-dev libx11-dev --install-recommends
sudo apt-get install libboost-all-dev libgoogle-glog-dev libprotobuf-dev protobuf-compiler
mkdir build
cd build
cmake ../
make



